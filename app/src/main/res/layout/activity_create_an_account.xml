<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="main2Activity"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.prasad.myapplication.LoggingActivity">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:layout_marginStart="11dp"
        android:id="@+id/editText"
        android:singleLine="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="SKIP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:id="@+id/but4"
        android:layout_below="@+id/but3"
        android:layout_alignStart="@+id/but2"
        android:textColor="@color/colorPrimary"
        android:layout_alignEnd="@+id/but3" />

    <Button
        android:text="LOGGING"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:id="@+id/but2"
        android:layout_below="@+id/editText3"
        android:layout_alignStart="@+id/but3"
        android:textColor="@color/colorPrimary"
        android:layout_alignEnd="@+id/but3" />

    <Button
        android:text="CREATE AN ACCOUNT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:id="@+id/but3"
        android:layout_marginEnd="22dp"
        android:textColor="@color/colorPrimary"
        android:layout_below="@+id/but2"
        android:layout_alignEnd="@+id/editText3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Password"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_below="@+id/editText"
        android:layout_alignStart="@+id/editText" />
</RelativeLayout>
